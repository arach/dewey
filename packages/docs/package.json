{
  "name": "@arach/dewey",
  "version": "0.1.0",
  "description": "Documentation toolkit with CLI and React components for agent-ready docs",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "bin": {
    "dewey": "./dist/cli/index.js"
  },
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./styles": "./dist/styles.css"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "vite build --watch",
    "build": "tsc && vite build && tsup src/cli/index.ts --format esm --out-dir dist/cli",
    "clean": "rm -rf dist",
    "lint": "eslint src"
  },
  "peerDependencies": {
    "react": "^18.0.0 || ^19.0.0",
    "react-dom": "^18.0.0 || ^19.0.0",
    "react-router-dom": "^6.0.0 || ^7.0.0"
  },
  "dependencies": {
    "chalk": "^5.3.0",
    "commander": "^12.0.0",
    "cosmiconfig": "^9.0.0",
    "gray-matter": "^4.0.3",
    "highlight.js": "^11.9.0",
    "lucide-react": "^0.400.0",
    "react-markdown": "^9.0.0",
    "rehype-slug": "^6.0.0",
    "remark-gfm": "^4.0.0",
    "zod": "^3.23.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0",
    "@vitejs/plugin-react": "^4.3.0",
    "tsup": "^8.0.0",
    "typescript": "^5.5.0",
    "vite": "^6.0.0",
    "vite-plugin-dts": "^4.0.0"
  },
  "keywords": [
    "documentation",
    "docs",
    "cli",
    "react",
    "components",
    "agents",
    "llm",
    "markdown"
  ],
  "author": "Arach Tchoupani",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/arach/dewey"
  }
}
